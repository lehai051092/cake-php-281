{
    "m2_cron_run": {
        "schedule": "*/5 * * * *",
        "command": "php bin/magento cron:run 2>&1 | grep -v 'Ran jobs by schedule' >> var/log/magento.cron.log",
        "container": "cli",
        "shell": "/bin/bash"
    },
    "clear_old_session": {
        "schedule": "0 2 * * *",
        "command": "find var/session/sess* -type f -ctime +1 -exec rm -- {} \\;",
        "container": "cli",
        "shell": "/bin/bash"
    }
}